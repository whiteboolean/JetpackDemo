<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/george"
        motion:constraintSetStart="@id/start"
        motion:duration="2000"
        motion:pathMotionArc="none"
        motion:motionInterpolator="easeInOut">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnClick motion:targetId="@id/imageView21" />
    </Transition>

    <ConstraintSet android:id="@+id/start">


        <Constraint
            android:layout_height="120dp"
            android:layout_width="120dp"
            android:id="@+id/imageView17"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_bias="0.273"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.172" />
        <Constraint
            android:layout_height="120dp"
            android:layout_width="120dp"
            android:id="@+id/imageView19"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.769"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.273" />
        <Constraint
            android:id="@+id/imageView20"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.578"
            motion:layout_constraintHorizontal_bias="0.172" />
        <Constraint
            android:id="@+id/imageView20"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.578"
            motion:layout_constraintHorizontal_bias="0.172" />
        <Constraint
            android:id="@+id/imageView21"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_bias="0.578"
            motion:layout_constraintHorizontal_bias="0.769" />
        <Constraint
            android:id="@+id/imageView21"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintVertical_bias="0.578"
            motion:layout_constraintHorizontal_bias="0.769" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/george"
        android:rotation="360">

        <Constraint
            motion:layout_constraintVertical_bias="0.398"
            android:layout_height="251dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.454"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="213dp"
            android:id="@+id/imageView21" />
        <Constraint
            android:id="@+id/imageView19"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.273"
            motion:layout_constraintHorizontal_bias="0.769"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0" />
        <Constraint
            android:id="@+id/imageView20"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.578"
            motion:layout_constraintHorizontal_bias="0.161"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0" />
        <Constraint
            android:id="@+id/imageView17"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.273"
            android:layout_marginStart="44dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0" />
    </ConstraintSet>
    <ConstraintSet
        android:id="@+id/peppa"
        motion:deriveConstraintsFrom="@+id/start" >
        <Constraint
            motion:layout_constraintVertical_bias="0.427"
            android:layout_height="120dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.461"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:id="@+id/imageView20"
            android:scaleX="3"
            android:scaleY="3" />
        <Constraint
            android:id="@+id/imageView19"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_height="120dp"
            android:layout_width="120dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.273"
            motion:layout_constraintHorizontal_bias="0.769"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0" />
        <Constraint
            android:id="@+id/imageView21"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:layout_height="120dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.578"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.769"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0" />
        <Constraint
            android:id="@+id/imageView17"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_height="120dp"
            android:layout_width="120dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintVertical_bias="0.273"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintHorizontal_bias="0.172"
            motion:layout_constraintStart_toStartOf="parent"
            android:alpha="0" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/peppa"
        motion:duration="2000"
        motion:motionInterpolator="easeInOut">
        <OnClick motion:targetId="@id/imageView20" />
    </Transition>
    <ConstraintSet
        android:id="@+id/side"
        motion:deriveConstraintsFrom="@+id/start" >
        <Constraint
            motion:layout_constraintVertical_bias="0.07"
            android:layout_height="120dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.172"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:id="@+id/imageView19" />
        <Constraint
            motion:layout_constraintVertical_bias="0.578"
            android:layout_height="120dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.172"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:id="@+id/imageView20" />
        <Constraint
            motion:layout_constraintVertical_bias="0.875"
            android:layout_height="120dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.172"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="120dp"
            android:id="@+id/imageView21" />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/start"
        motion:constraintSetEnd="@+id/side" >
        <OnSwipe motion:dragDirection="dragStart" />
    </Transition>
    <ConstraintSet
        android:id="@+id/hide"
        motion:deriveConstraintsFrom="@+id/side" />
    <Transition
        motion:constraintSetStart="@+id/side"
        motion:constraintSetEnd="@+id/hide" >
        <KeyFrameSet >
            <KeyAttribute
                motion:motionTarget="@+id/imageView17"
                motion:framePosition="100"
                android:translationX="-300dp" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView17"
                motion:framePosition="0"
                android:translationX="30dp" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView19"
                motion:framePosition="100"
                android:translationX="-300dp" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView20"
                motion:framePosition="100"
                android:translationX="-300dp" />
            <KeyAttribute
                motion:motionTarget="@+id/imageView21"
                motion:framePosition="100"
                android:translationX="-400dp" />
        </KeyFrameSet>
        <OnSwipe motion:dragDirection="dragStart" />
    </Transition>
</MotionScene>